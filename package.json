{
  "name": "ts-to-effect-schema",
  "description": "Generate @effect/schema schemas from TypeScript types/interfaces",
  "version": "0.0.11",
  "homepage": "https://github.com/daotl/ts-to-effect-schema",
  "bugs": "https://github.com/daotl/ts-to-effect-schema/issues",
  "repository": "daotl/ts-to-effect-schema",
  "keywords": [
    "effect",
    "schema",
    "typescript",
    "generator",
    "oclif"
  ],
  "author": "DAOT Labs",
  "types": "lib/index.d.ts",
  "main": "lib/index.js",
  "bin": {
    "ts-to-effect-schema": "bin/run"
  },
  "scripts": {
    "build": "rimraf lib && tsc -p tsconfig.package.json",
    "prepublishOnly": "pnpm test && rimraf lib && pnpm build",
    "lint": "npx biome format . && npx biome check .",
    "lint:fix": "npx biome format --write . && npx biome check --apply .",
    "test": "jest",
    "test:ci": "jest --ci --coverage && pnpm gen:all && tsc --noEmit",
    "gen:all": "./bin/dev --all",
    "gen:example": "./bin/dev --config example",
    "gen:config": "./bin/dev --config config"
  },
  "files": [
    "LICENSE",
    "README.md",
    "lib",
    "bin"
  ],
  "license": "MIT",
  "resolutions": {
    "@effect/schema": "^0.42.0",
    "effect": "2.0.0-next.47"
  },
  "dependencies": {
    "@effect/schema": "^0.42.0",
    "@oclif/core": "^2.8.2",
    "@oclif/plugin-help": "^5.2.9",
    "@typescript/vfs": "^1.4.0",
    "async": "^3.2.4",
    "case": "^1.6.3",
    "chokidar": "^3.5.3",
    "effect": "2.0.0-next.47",
    "fs-extra": "^9.1.0",
    "inquirer": "^8.2.0",
    "lodash": "^4.17.21",
    "lz-string": "^1.5.0",
    "ora": "^5.4.0",
    "prettier": "2.3.0",
    "rxjs": "^7.8.0",
    "slash": "^3.0.0",
    "threads": "^1.7.0",
    "tslib": "^2.5.0",
    "tsutils": "^3.21.0",
    "typescript": "5.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-typescript": "^7.21.4",
    "@biomejs/biome": "^1.2.2",
    "@daotl/biome-config": "^0.0.1",
    "@types/async": "^3.2.19",
    "@types/fs-extra": "^9.0.9",
    "@types/inquirer": "^8.2.6",
    "@types/jest": "^29.5.1",
    "@types/lodash": "^4.14.194",
    "@types/lz-string": "^1.5.0",
    "@types/node": "^14.18.43",
    "auto-changelog": "^2.4.0",
    "babel-jest": "^29.5.0",
    "codecov": "^3.8.3",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "oclif": "^3.8.1",
    "pretty-quick": "^3.1.3",
    "rimraf": "^5.0.0",
    "ts-node": "^10.9.1",
    "type-fest": "^3.9.0"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/*.test.ts"
    ]
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "trailingComma": "es5"
  },
  "oclif": {
    "bin": "ts-to-effect-schema",
    "dirname": "ts-to-effect-schema",
    "commands": "./lib/commands",
    "default": "generate",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": " "
  }
}
