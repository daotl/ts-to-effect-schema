{
  "name": "ts-to-effect-schema",
  "description": "Generate @effect/schema schemas from TypeScript types/interfaces",
  "version": "0.0.1",
  "homepage": "https://github.com/daotl/ts-to-effect-schema",
  "bugs": "https://github.com/daotl/ts-to-effect-schema/issues",
  "repository": "daotl/ts-to-effect-schema",
  "keywords": [
    "effect",
    "schema",
    "typescript",
    "generator",
    "oclif"
  ],
  "author": "DAOT Labs",
  "types": "lib/index.d.ts",
  "main": "lib/index.js",
  "bin": {
    "ts-to-effect-schema": "bin/run"
  },
  "scripts": {
    "build": "rimraf lib && tsc -p tsconfig.package.json",
    "prepublishOnly": "pnpm test:ci && rimraf lib && pnpm build",
    "lint": "npx rome format . && npx rome check .",
    "lint:fix": "npx rome format --write . && npx rome check --apply .",
    "test": "jest",
    "test:ci": "jest --ci --coverage && pnpm gen:all && tsc --noEmit",
    "gen:all": "./bin/dev --all",
    "gen:example": "./bin/dev --config example",
    "gen:config": "./bin/dev --config config"
  },
  "files": [
    "LICENSE",
    "README.md",
    "ts-to-effect-schema-logo.svg",
    "lib",
    "bin"
  ],
  "license": "MIT",
  "resolutions": {
    "@effect/data": "^0.12.2",
    "@effect/io": "^0.24.2",
    "@effect/schema": "^0.15.0"
  },
  "dependencies": {
    "@effect/data": "^0.12.2",
    "@effect/io": "^0.24.2",
    "@effect/schema": "^0.14.1",
    "@oclif/core": "^2.8.2",
    "@oclif/plugin-help": "^5.2.9",
    "@oclif/plugin-plugins": "^2.4.6",
    "@typescript/vfs": "^1.3.5",
    "async": "^3.2.0",
    "case": "^1.6.3",
    "chokidar": "^3.5.1",
    "fs-extra": "^9.1.0",
    "inquirer": "^8.2.0",
    "lodash": "^4.17.21",
    "lz-string": "^1.4.4",
    "ora": "^5.4.0",
    "prettier": "2.2.1",
    "rxjs": "^7.4.0",
    "slash": "^3.0.0",
    "threads": "^1.7.0",
    "tslib": "^2.3.1",
    "tsutils": "^3.21.0",
    "typescript": "5.0.4",
    "zod": "^3.11.6"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@daotl/rome-config": "0.0.1-alpha.4",
    "@types/async": "^3.2.6",
    "@types/fs-extra": "^9.0.9",
    "@types/inquirer": "^8.1.3",
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.168",
    "@types/lz-string": "^1.3.34",
    "@types/node": "^12.7.11",
    "auto-changelog": "^2.2.1",
    "babel-jest": "^27.5.1",
    "codecov": "^3.8.1",
    "husky": "^5.2.0",
    "jest": "^27.5.1",
    "oclif": "^3",
    "pretty-quick": "^3.1.0",
    "rimraf": "^3.0.2",
    "rome": "^12.0.0",
    "ts-node": "^10.9.1",
    "type-fest": "^3.8.0"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/*.test.ts"
    ]
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "trailingComma": "es5"
  },
  "oclif": {
    "bin": "ts-to-effect-schema",
    "dirname": "ts-to-effect-schema",
    "commands": "./lib/commands",
    "default": "generate",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " "
  }
}
